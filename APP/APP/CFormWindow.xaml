<Window x:Class="APP.CFormWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:APP"
        mc:Ignorable="d"
        Title="CFormWindow" Height="200" Width="400">
    <Grid>
        <Button Content="Submit" HorizontalAlignment="Left" Height="39" Margin="64,130,0,0" VerticalAlignment="Top" Width="102" Click="btnAddSubmit"/>
        <Button Content="Exit" HorizontalAlignment="Left" Height="39" Margin="255,130,0,0" VerticalAlignment="Top" Width="102" Click="btnAddExit"/>
        <TextBox Name="TextBox1" HorizontalAlignment="Left" Height="19" Margin="70,18,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="104"/>
        <TextBox Name="TextBox2" HorizontalAlignment="Left" Height="19" Margin="70,53,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="104"/>
    </Grid>
</Window>
